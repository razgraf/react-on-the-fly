{
	"name": "react-on-the-fly",
	"displayName": "React On The Fly",
	"description": "Turn boilerplate into a new component and auto-import it on the fly in React.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:react-on-the-fly.hello",
		"onCommand:react-on-the-fly.otfc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-on-the-fly.hello",
				"title": "hello"
			},
			{
				"command": "react-on-the-fly.otfc",
				"title": "On the fly component (opinionated)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}